class Car {
    private model: string;
    protected brand: string;
    public readonly year: number; // Adding readonly modifier

    constructor(model: string, brand: string, year: number) {
        this.model = model;
        this.brand = brand;
        this.year = year; // Readonly property can be assigned in the constructor
    }

    getCarInfo(): string {
        return `${this.brand}, ${this.year}`;
    }
}

const obj = new Car("abc", "ford", 2022);
obj.year=2020   // compilation error (readonly modifier)
console.log(`Car Info: ${obj.getCarInfo()}`);

OUTPUT:  Car Info: ford, 2022
