class Car{
    model: String
    brand: String
    year:number
    constructor(model:String,brand:String,year:number){
        this.model=model;
        this.brand=brand;
        this.year=year;
    }

    getCarInfo(){
        return `${this.model},${this.brand},${this.year}`
    }
}
const obj=new Car("abc","ford",2022)
console.log(`car info: ${obj.getCarInfo()}`)

OUTPUT:
car info: abc,ford,2022
