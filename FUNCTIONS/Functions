// Function with void return type
function logMessage(message: string): void {
    console.log(message);
    // No return statement or returning undefined (implicitly)
}

// Function with number or string return type
function concatenate(a: number, b: number): number | string {
    if (a === b) {
        return a * 2; // Returns a number
    } else {
        return `Concatenated: ${a}${b}`; // Returns a string
    }
}

logMessage("Hello, void function!"); 
Output: Hello, void function!

const result1: number | string = concatenate(5, 5);
console.log(result1);
Output: 10

const result2: number | string = concatenate(3, 7);
console.log(result2); 
Output: Concatenated: 37
