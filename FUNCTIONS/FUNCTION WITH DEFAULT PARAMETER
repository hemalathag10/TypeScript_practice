// FUNCTION WITH DEFAULT PARAMETER
function calculateProductCost(productName: string, price: number, discount: number = 5): number {
    const discountedPrice = price - (price * discount) / 100;
    console.log(discount)
    return discountedPrice;
}

const regularPrice = calculateProductCost("Laptop", 1200);
console.log(`Regular Price: $${regularPrice}`); // Output: Regular Price: $1200

const discountedPrice = calculateProductCost("Smartphone", 800, 10);
console.log(`Discounted Price: $${discountedPrice}`); // Output: Discounted Price: $720
